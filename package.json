{
  "name": "did-hybrid-app",
  "version": "0.0.1",
  "author": "Bernhard Fuchs",
  "scripts": {
    "ng": "ng",
    "start": "npx ionic serve",
    "build": "npx ionic build",
    "add:android": "npx ionic capacitor add android",
    "add:ios": "npx ionic capacitor add ios",
    "copy:android": "npx ionic capacitor copy android",
    "copy:ios": "npx ionic capacitor copy ios",
    "open:android": "npx ionic capacitor open android",
    "open:ios": "npx ionic capacitor open ios",
    "run:android": "npx ionic capacitor run android",
    "run:ios": "npx ionic capacitor run ios",
    "sync:android": "npx ionic capacitor sync android",
    "sync:ios": "npx ionic capacitor sync ios",
    "update:android": "npx ionic capacitor update android",
    "update:ios": "npx ionic capacitor update ios",
    "cocoapods:update": "pod repo update",
    "format:fix": "pretty-quick",
    "format:commit": "pretty-quick --staged",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@capacitor/android": "^1.3.0",
    "@capacitor/core": "1.3.0",
    "@capacitor/ios": "^1.3.0",
    "@ionic-native/barcode-scanner": "^5.16.0",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.7.1",
    "core-js": "^2.5.4",
    "phonegap-plugin-barcodescanner": "^8.1.0",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.801.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/core": "~8.1.2",
    "@angular-devkit/schematics": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@capacitor/cli": "1.3.0",
    "@ionic/angular-toolkit": "~2.0.0",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.9",
    "ionic": "^5.4.5",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:commit && npm run lint"
    }
  },
  "cordova": {
    "plugins": {
      "phonegap-plugin-barcodescanner": {}
    }
  }
}
